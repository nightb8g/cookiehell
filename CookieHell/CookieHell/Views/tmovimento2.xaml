<navigation:Page x:Class="CookieHell.tmovimento2" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           Title="tmovimento2 Page">
    <Grid x:Name="LayoutRoot">
        <ScrollViewer x:Name="teste" HorizontalScrollBarVisibility="Hidden" VerticalAlignment="Bottom" VerticalScrollBarVisibility="Hidden"  BorderBrush="{x:Null}" HorizontalAlignment="Left" >
            <Image  x:Name="bg" Source="/CookieHell;component/img/bg_game_trial.png" Stretch="None" HorizontalAlignment="Left" VerticalAlignment="Bottom" >
                <Image.RenderTransform>
                    <CompositeTransform x:Name="bg_pontos"/>
                </Image.RenderTransform>
            </Image>

        </ScrollViewer>
        <Image Margin="0,0,0,60" VerticalAlignment="Bottom" Source="/CookieHell;component/img/c_go1.png" HorizontalAlignment="Left" >
            <Image.RenderTransform>
                <CompositeTransform x:Name="player_pontos"/>
            </Image.RenderTransform>
        </Image>
    </Grid>
</navigation:Page>

