<navigation:Page x:Class="CookieHell.Game" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="Game Page">
	<navigation:Page.Resources>
		<Storyboard x:Name="c_move">
			<DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Canvas.Left)" Storyboard.TargetName="c_go1">
				<EasingDoubleKeyFrame KeyTime="0" Value="191"/>
				<EasingDoubleKeyFrame KeyTime="0:0:1" Value="201"/>
			</DoubleAnimationUsingKeyFrames>
		</Storyboard>
	</navigation:Page.Resources>
    <Grid x:Name="LayoutRoot">
        <Canvas>
            <Image x:Name="go1" HorizontalAlignment="Left" Height="100" VerticalAlignment="Bottom" Width="100" Canvas.Left="149" Canvas.Top="37" Source="/CookieHell;component/img/c_go1.png"/>
            <Image x:Name="go2" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Canvas.Left="254" Canvas.Top="37" Source="/CookieHell;component/img/c_go2.png"/>
            <Image Height="100" Canvas.Left="44" Canvas.Top="37" Width="100" Source="/CookieHell;component/img/c_jump.png"/>
            <Image x:Name="c_go1" Height="100" Canvas.Left="191" Canvas.Top="297" Width="50" Source="/CookieHell;component/img/c_go2.png" RenderTransformOrigin="0.5,0.5">
            	<Image.RenderTransform>
            		<CompositeTransform/>
            	</Image.RenderTransform>
            </Image>
        </Canvas>
        <Image x:Name="c_go2" Source="/CookieHell;component/img/c_go2.png" RenderTransformOrigin="0.5,0.5" Margin="365,10,0,0" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="50">
        	<Image.RenderTransform>
        		<CompositeTransform/>
        	</Image.RenderTransform>
        </Image>

    </Grid>
</navigation:Page>

